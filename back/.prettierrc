{
  "trailingComma": "none",
  "arrowParens": "avoid",
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "bracketSameLine": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/*.routes.*",
    "/auth/",
    "/exersice/",
    "/middleware/",
    "/user/",
    "/workout/",
    "/log/",
    "/prisma.js",
    "^../(.*)",
    "^./(.*)"
  ]
}
