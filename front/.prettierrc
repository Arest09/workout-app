{
  "trailingComma": "none",
  "arrowParens": "avoid",
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "bracketSameLine": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/ui/",
    "/hooks/",
    "/pages/",
    "/components/",
    "/services/",
    "/hoc/",
    "/context",
    "^../(.*)",
    "^./(.*)",
    "/module.scss/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
